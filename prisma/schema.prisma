generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
    id        String   @id @default(cuid())
    username  String   @unique
    password  String?
}

model Produk {
    id      String  @id @default(cuid())
    image   String
    title   String
    price   Int
}

model Service {
    id      String  @id @default(cuid())
    image   String
    title   String
}